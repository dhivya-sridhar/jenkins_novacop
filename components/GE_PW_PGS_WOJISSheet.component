<apex:component controller="GE_HQ_WOJISSheetController" access="global" >
    <apex:attribute name="woId" type="String" description="Work Order Id" assignTo="{!WOId}" />
    
    <div class="title" >Job Information Sheet</div>
    <div style="clear:both;height:10px;" ></div>
    <div class="sectionTitle"  style="font-size:18px; font-weight:bold;">Customer Details</div>
    <table cellspacing="4px" style="border: 1px solid black;" width="100%">
        <tr>
            <th align="right" style="font-size:11px; vertical-align:top;background-color:#D3D3D3;">Customer Name:</th>
            <td colspan="3" style="font-size:11px; vertical-align:top;" ><apex:outputText value="{!WorkOrder.SVMXC__Company__r.Name}" /></td>
        </tr>
        <tr>
            <th align="right" style="font-size:11px; vertical-align:top;background-color:#D3D3D3;">Customer Site Name:</th>
            <td colspan="3" style="font-size:11px; vertical-align:top;" ><apex:outputText value="{!WorkOrder.SVMXC__Case__r.GE_SM_HQ_Site_Name__c}" /></td>
        </tr>
        <tr>
            <th align="right" style="font-size:11px; vertical-align:top;background-color:#D3D3D3;">Customer Site Address:</th>
            <td colspan="3" style="font-size:11px; vertical-align:top;" ><apex:outputText value="{!WorkOrder.SVMXC__City__c}" /> / <apex:outputText value="{!WorkOrder.SVMXC__State__c}" rendered="{!WorkOrder.SVMXC__State__c != null}" /> / <apex:outputText value="{!WorkOrder.SVMXC__Country__c}" /> / <apex:outputText value="{!WorkOrder.SVMXC__Zip__c}" /></td>
        </tr>
        <tr>
            <th align="right" style="font-size:11px; vertical-align:top;background-color:#D3D3D3;">Ship To Address:</th>
            <td colspan="3" style="font-size:11px; vertical-align:top;" ><apex:outputText value="{!WorkOrder.GE_SM_HQ_Ship_to_Address__c}" /></td>
        </tr>
        <tr>
            <th width="1.8in"  align="right" style="font-size:11px; vertical-align:top;background-color:#D3D3D3;">Cust Rep Name:</th>
            <td width="2in" style="font-size:11px; vertical-align:top;"><apex:outputText value="{!WorkOrder.SVMXC__Contact__r.Name}" /></td>
            <th width="1.5in"  align="right" style="font-size:11px; vertical-align:top;background-color:#D3D3D3;">Rep Title:</th>
            <td width="2in" style="font-size:11px; vertical-align:top;"><apex:outputText value="{!WorkOrder.GE_SM_HQ_Site_CRTitle__c}" /></td>
        </tr>
        <tr>
            <th width="1.8in"  align="right" style="font-size:11px; vertical-align:top;background-color:#D3D3D3;">Cust Rep Mobile:</th>
            <td width="2in" style="font-size:11px; vertical-align:top;"><apex:outputText value="{!WorkOrder.GE_SM_HQ_Site_CRMobile__c}" /></td>
            <th width="1.5in"  align="right" style="font-size:11px; vertical-align:top;background-color:#D3D3D3;">E-mail:</th>
            <td width="2in" style="font-size:11px; vertical-align:top;"><apex:outputText value="{!WorkOrder.GE_SM_HQ_Site_CREmail__c}" /></td>
        </tr>
    </table>
    <div style="clear:both;height:10px" ></div>
    <div class="sectionTitle"  style="font-size:18px; font-weight:bold;">Equipment Details</div>
