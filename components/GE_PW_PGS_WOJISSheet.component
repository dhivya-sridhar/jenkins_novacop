            <td width="2in" style="font-size:11px; vertical-align:top;"><apex:outputText value="{!WorkOrder.SVMXC__Case__r.GE_SM_HQ_Contact_Visa_Processing__c}" /></td>
            <th width="1.5in"  align="right" style="font-size:11px; vertical-align:top;background-color:#D3D3D3;">Tel No:</th>
